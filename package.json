{
  "name": "wind-barbs",
  "version": "1.0.7",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/wojonet/wind-barbs"
  },
  "description": "A small utility to generate wind barb SVGs",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./browser": {
      "import": "./dist/global/index.esm.js",
      "default": "./dist/global/index.umd.js",
      "types": "./dist/global/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.esm.js",
      "default": "./dist/react/index.umd.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "keywords": [
    "wind",
    "barbs",
    "svg",
    "react",
    "weather"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack",
    "compile": "tsc",
    "test": "serve ./",
    "clean": "rimraf ./dist"
  },
  "author": "Alec Wojciechowski",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "@types/react": ">=0.14.27",
    "react": ">=0.14.27"
  },
  "optionalPeerDependencies": {
    "@types/react": ">=0.14.27",
    "react": ">=0.14.27"
  }
}
