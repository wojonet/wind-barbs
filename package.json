{
  "name": "wind-barbs",
  "version": "1.0.6",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/wojonet/wind-barbs"
  },
  "description": "A small utility to generate wind barb SVGs",
  "main": "./dist/bundle/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react/index.cjs.js",
      "import": "./dist/react/index.esm.js",
      "default": "./dist/react/index.umd.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "keywords": [
    "wind",
    "barbs",
    "svg",
    "react",
    "weather"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack",
    "build:rn": "tsc --project tsconfig.react-native.json",
    "webpack": "webpack",
    "compile": "tsc",
    "test": "serve ./",
    "clean": "rimraf ./dist"
  },
  "author": "Alec Wojciechowski",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.12",
    "@types/react-test-renderer": "^19.0.0",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "@types/react": ">=0.14.27",
    "react": ">=0.14.27",
    "react-native-svg": ">=15.1.0"
  },
  "optionalPeerDependencies": {
    "@types/react": ">=0.14.27",
    "react": ">=0.14.27",
    "react-native-svg": ">=15.1.0"
  }
}
